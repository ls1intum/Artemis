{
    "name": "artemis_cypress",
    "description": "Cypress tests for Artemis",
    "private": true,
    "license": "MIT",
    "cacheDirectories": [
        "node_modules"
    ],
    "devDependencies": {
        "@4tw/cypress-drag-drop": "2.2.4",
        "@types/node": "20.2.5",
        "cypress": "12.16.0",
        "cypress-cloud": "1.8.2",
        "cypress-file-upload": "5.0.8",
        "cypress-wait-until": "2.0.0",
        "typescript": "4.9.5",
        "uuid": "9.0.0",
        "wait-on": "7.0.1"
    },
    "overrides": {
        "semver": "7.5.3",
        "word-wrap": "1.2.3",
        "debug": "4.3.4"
    },
    "scripts": {
        "cypress:open": "cypress open",
        "cypress:run": "cypress run --browser=chrome",
        "cypress:record:mysql": "npx cypress-cloud run --record --ci-build-id \"${SORRY_CYPRESS_BRANCH_NAME} #${SORRY_CYPRESS_BUILD_ID} (MySQL)\"",
        "cypress:record:postgresql": "npx cypress-cloud run --record --ci-build-id \"${SORRY_CYPRESS_BRANCH_NAME} #${SORRY_CYPRESS_BUILD_ID} (PostgreSQL)\"",
        "update": "npm-upgrade"
    }
}
